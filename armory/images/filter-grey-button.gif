<body class="errorpage">
<script type="text/javascript">

var region="";
if(document.location.href.indexOf("wow-europe.com")!=-1 || document.location.href.indexOf("80.239.186.40")!=-1 || document.location.href.indexOf("eu.blizzard.net")!=-1 || document.location.href.indexOf("preprod")!=-1 || document.location.href.indexOf("eu.wowarmory.com")!=-1) //Checking for Europe settings
    region="EU"
else if(document.location.href.indexOf("armory.worldofwarcraft.co.kr")!=-1){//change by akim (add KR)
    region="KR";
}
else
    region="US"

function getWoWUrl() {
	if (region == "US")
		location.href = "../../www.worldofwarcraft.com/default.htm";
	else
		location.href = "../../www.wow-europe.com/default.htm";
}
	
</script>
<div class="container">
<h1>File not found.</h1>
<ul>
<li>
<a href="../default.htm">Return to the Armory</a>
</li>
<li>
